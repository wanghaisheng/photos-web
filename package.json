  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev --workspace=apps/backend\" \"npm run dev --workspace=apps/web\"",
    "build": "concurrently \"npm run build --workspace=apps/backend\" \"npm run build --workspace=apps/web\"",
    "start": "concurrently \"npm run start --workspace=apps/backend\" \"npm run start --workspace=apps/web\"",
    "test": "concurrently \"npm run test --workspace=apps/backend\" \"npm run test --workspace=apps/web\"",
    "lint": "concurrently \"npm run lint --workspace=apps/backend\" \"npm run lint --workspace=apps/web\"",
    "prettier": "concurrently \"npm run prettier --workspace=apps/backend\" \"npm run prettier --workspace=apps/web\"",
    "prettier:fix": "concurrently \"npm run prettier:fix --workspace=apps/backend\" \"npm run prettier:fix --workspace=apps/web\"",
    "type-check": "concurrently \"npm run type-check --workspace=apps/backend\" \"npm run type-check --workspace=apps/web\""
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
